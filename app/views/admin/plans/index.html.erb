<h1 class="page-header">デートプラン一覧</h1>

<table class="table table-bordered table-hover table-responsive">
	<thead>
		<tr class="success">
			<th>ID</th>
			<th>タイトル</th>
			<th>登録日</th>
			<th>更新日</th>
		</tr>
	</thead>
	<tbody>
		<% @plans.each do |plan| %>
	    <tr>
				<td><%= link_to plan.id , edit_staff_plan_path(plan.id) %></td>
				<td><%= plan.title %> </td>
				<td><%= plan.created_at %></td>
				<td><%= plan.updated_at %> </td>
	    </tr>
		<% end %>
	</tbody>
</table>
